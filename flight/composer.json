{
    "name": "digipay/flightphp",
    "description": "DigiPay Payment Gateway - FlightPHP Version",
    "type": "project",
    "require": {
        "php": "^8.2",
        "mikecao/flight": "^3.17",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
